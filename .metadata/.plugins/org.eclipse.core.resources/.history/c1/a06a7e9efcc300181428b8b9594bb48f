package Strings;

import java.math.BigInteger;

public class Quiz1 {
	public static void main(String[] args) {
		int x = 0;

		int[] nums = { 1, 2, 3, 5 };

		for (int i : nums)
			switch (i) {
			case 1:
				x += i;
				break;
			case 5:
				x += i;
				break;
			default:
				x += i;
				break;
			case 2:
				x += i;
				break;
			}
		System.out.println(x);// 11
		/**
		 * When a case is matched in switch, its code and all the subsequent case code
		 * will run unless break is encountered.
		 */
		 x = 0;
		for (int i : nums)
			switch (i) {
			case 1:
				x += i;
			case 5:
				x += i;
			default:
				x += i;
			case 2:
				x += i;
			}
		System.out.println(x);// 27
		Integer i1 = -128;
        Integer i2 = -128;
        Integer i3 = 128;
        Integer i4 = 128;
        System.out.println((i1==i2) + "," + (i3==i4));
        
		
		
	}
}
