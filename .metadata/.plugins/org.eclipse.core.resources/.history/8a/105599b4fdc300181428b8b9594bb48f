package inheritence;

 interface InterfaceA {
	 default String getMessage() {
	        return "Hola";
	    }
}
public class ClassA {
		public String getMessage() {
	        return "Hello";
		}
}

class ClassDemo extends ClassA implements InterfaceA {
	public static void main(String[] args) {
        System.out.println(new ClassDemo().getMessage());
    }
	
}

class ClassDemo1  implements InterfaceA {
	public static void main(String[] args) {
        System.out.println(new ClassDemo().getMessage());
    }
	
}